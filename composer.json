{
  "name": "mapbender/configurator",
  "description": "Mapbender configuration module",
  "license": "MIT",
  "authors": [
    {
      "name": "Andriy Oblivantsev",
      "email": "eslider@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "predis/predis": "~1.0",
    "satooshi/php-coveralls": "~0.6"
  },
  "require": {
    "symfony/dependency-injection": ">=2.3",
    "symfony/options-resolver": "*",
    "mapbender/search": "dev-master",
    "wheregroup/wfs-driver": "dev-master",
    "zumba/json-serializer": "*"
  },
  "config": {
    "bin-dir": "bin",
    "platform": {
      "php": "5.3.17"
    }
  },
  "autoload": {
    "psr-0": {
      "Mapbender\\ConfiguratorBundle": "."
    }
  },
  "target-dir": "Mapbender/ConfiguratorBundle"
}
